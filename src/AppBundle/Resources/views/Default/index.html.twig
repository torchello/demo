{% extends 'base.html.twig' %}

{% block body %}
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Country</th>
            <th>State</th>
        </tr>
    </thead>
    <tbody>
        {% for user in users %}
            <tr>
                <td>{{ user.id }}</td>
                <td>{{ user.country }}</td>
                <td>{{ user.state }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>
{% endblock %}
